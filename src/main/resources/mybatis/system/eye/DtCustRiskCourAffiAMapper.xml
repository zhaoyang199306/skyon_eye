<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.skyon.project.system.mapper.eye.DtCustRiskCourAffiAMapper">
	

    <resultMap id="DtCustRiskCourAffiAResult" type="DtCustRiskCourAffiA">
        <result property="lpOrgNo" column="LP_ORG_NO"/>
        <result property="custNo" column="CUST_NO"/>
        <result property="custName" column="CUST_NAME"/>

        <result property="caseReason" column="case_reason"/>
        <result property="plaintiff" column="plaintiff"/>
        <result property="defendant" column="defendant"/>
        <result property="courDt" column="cour_dt"/>
        <result property="caseNum" column="case_num"/>
        <result property="courtName" column="court_name"/>

        <result property="etlTimestamp" column="ETL_TIMESTAMP"/>
        <result property="partYmd" column="PART_YMD"/>
        <result property="partYm" column="PART_YM"/>
        <result property="monitorType" column="MONITOR_TYPE"/>
    </resultMap>
	
	<select id="selectDtCustRiskCourAffiA" parameterType="String" resultType="DtCustRiskCourAffiA" resultMap="DtCustRiskCourAffiAResult">
        select * from DT_CUST_RISK_COUR_AFFI_A
        where cust_no = #{custNo}
    </select>




</mapper> 